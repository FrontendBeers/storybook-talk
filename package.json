{
  "name": "storybook-talk",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "eslint": "^5.12.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-scripts": "2.1.8",
    "styled-components": "^4.1.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
